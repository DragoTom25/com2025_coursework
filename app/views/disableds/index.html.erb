<p id="notice"><%= notice %></p>

<h1>Disableds</h1>

<table>
  <thead>
    <tr>
      <th>Car plate</th>
      <th>Disability</th>
      <th>Name</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disableds.each do |disabled| %>
      <tr>
        <td><%= disabled.car_plate %></td>
        <td><%= disabled.disability %></td>
        <td><%= disabled.name %></td>
        <td><%= disabled.gender %></td>
        <td><%= link_to 'Show', disabled %></td>
        <td><%= link_to 'Edit', edit_disabled_path(disabled) %></td>
        <td><%= link_to 'Destroy', disabled, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Disabled', new_disabled_path %>
