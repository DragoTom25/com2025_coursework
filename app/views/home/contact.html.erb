<header>
  <div class="topnav">
  <a href="http://localhost:3000">Home</a>
  <b><a href="#booking">Book Now</a></b>
  <a href="http://localhost:3000/about">About us</a>
  <a href="#resources">Resources</a>
  <a class="active" href="http://localhost:3000/contact">Contact</a>
</div>
</header>

<h1>
  <center><%= t('.ctitle') %> </center>
  <br>
  <h2> <%= t('.contact_us') %> </h2>
</h1>

<%= form_tag request_contact_path, method: 'post' do %>
  <%= label_tag :name, t('.name') %>
  <%= text_field_tag :name, '', title: t('.enter_name'), size: 50 %>
  <%= label_tag :email, t('.email') %>
  <%= email_field_tag :email, '', title: t('.enter_email') %>
  <%= label_tag :telephone, t('.telephone') %>
  <%= telephone_field_tag :telephone, '', title: t('.enter_telephone') %>
  <%= label_tag :message, t('.message') %>
  <%= text_field_tag :message, '', title: t('.enter_message'), size: 100 %>
  <%= submit_tag t('.submit') %>
<% end %>

<body>
  <div class="footer">
<img src="https://oneparkingsolution.co.uk/wp-content/uploads/2016/07/ops_logo.jpg" style="width:100px;height:50px">
</div>
</body>
